{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ."
  },
  "nodeGypRebuild": "true",
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^20.38.4",
    "electron-rebuild": "^1.8.2"
  },
  "dependencies": {
    "midi": "^0.9.5",
    "robotjs": "^0.5.1"
  },
  "build": {
    "asarUnpack": [
      "**/app/node_modules/midi/*",
      "**/app/node_modules/robotjs/*"
    ]
  }
}
